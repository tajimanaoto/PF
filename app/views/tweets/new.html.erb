<h1>新規投稿</h1>
<%= form_with model:@tweet, local:true do |f| %>

　<div class="form-group">
    <label class="user_image">Image</label>
    <%= f.attachment_field :image, class:"form-control-file" %>
  </div>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>紹介文</h4>
  <%= f.text_area :description %>

  <%= f.submit '投稿' %>

<% end %>